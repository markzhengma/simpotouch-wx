.calendar-eventlist-head {
  font-size: large;
  font-weight: 800;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 40px;
  margin: 0 30px;
}
.calendar-datechange-btn {
  color: papayawhip;
  background-color: lightslategray;
  padding: 6px;
  border-radius: 4px;
  box-shadow: 2px 2px 10px rgb(82, 82, 82);
}
.calendar-eventlist-body {
  height: 90vh;
  overflow-y: scroll;
}
.calendar-eventlist-single {
  margin: 10px;
  background-color: papayawhip;
  color: gray;
  border-radius: 4px;
}
.calendar-eventlist-title {
  font-size: 20px;
  background-color: lightslategray;
  color: papayawhip;
  border-radius: 4px 4px 0 0;
  padding: 10px;
}
.calendar-eventlist-time {
  padding: 10px;
}
.calendar-eventlist-location {
  padding: 10px;
}